{
  "name": "api",
  "version": "1.0.0",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "node --watch ./src/server.js",
    "start": "node ./src/server.js",
    "backup": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/backup.ps1 -Mode quick",
    "backup:full": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/backup.ps1 -Mode full",
    "backups:list": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/backups-list.ps1",
    "restore": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/restore.ps1",
    "restore:latest": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/restore-latest.ps1",
    "restore:interactive": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/restore-interactive.ps1"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.3.8",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "pdfkit": "^0.17.2"
  }
}
